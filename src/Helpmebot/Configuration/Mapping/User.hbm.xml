<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Helpmebot" namespace="Helpmebot.Model">
  <class name="User">
    <id column="user_id" name="id"></id>
    <property column="user_nickname" name="nickname" />
    <property column="user_username" name="username"/>
    <property column="user_hostname" name="hostname"/>
    <property column="user_accesslevel" name="accessLevel" />
    <property column="user_lastmodified" name="lastModified" />
  </class>  
</hibernate-mapping>